#include <mega168p.h>

eeprom char Parameter_bank[44][24]=
                               {
                                {0x00},                 //0 - этот символ мы пропускаем
                                {0x56},                 //1 - идентификационный код производителя
                                {0x03},                 //2 - код тип устройства производителя
                                {0x03},                 //3 - необходимое количество преамбул
                                {0x01},                 //4 - ревизия универсальных команд
                                {0x01},                 //5 - ревизия специфических команд
                                {0x01},                 //6 - ревизия ПО
                                {0x21},                 //7 - ревизия аппаратного обеспечения
                                {0x02},                 //8 - флаги функций устройства (Н тип)
                                {0x14, 0x06, 0x0b},     //9 - идентификационное число устройства (В тип)
                                {0x6d},                 //10 - код единиц измерения первичной переменной
                                {0x00},                 //11 - первичная перменная (F тип)
                                {0x00},                 //12 - ток(мА)
                                {0x00},                 //13 - процент диапазона
                                {0x02},                 //14 - адрес опроса
                                {0x00},                 //15 - тэг, 8 символов (А тип)
                                {0x00},                 //16 - сообщение, 32 символа (А тип)
                                {0x00},                 //17 - дескриптор тэга, 16 символов (А тип)
                                {0x00},                 //18 - дата (D тип)
                                {0x01, 0x02, 0x03},     //19 - серийный номер  чувствительного элемента
                                {0x42, 0x48},           //20 - верхний предел датчика (F тип)
                                {0x00},                 //21 - нижний предел датчика (F тип)
                                {0x3C, 0x23, 0xD7, 0x0A},//22 - минимальный шаг (F тип)
                                {0x00},                  //23 - код выбора оповещения
                                {0x00},                  //24 - код функции преобразования
                                {0x41, 0xA0},            //25 - верхний диапазон измерений (F тип)
                                {0x00},                  //26 - нижний диапазон  измерений (F тип)
                                {0xf0},                  //27 - код защиты от записи
                                {0x0f},                  //28 - код индивидуального ярлыка дистрибьютора
                                {0x15, 0x06, 0x0b},      //29 - технологический номер
                                {0x00},                  //30 - байт контроля ЭСППЗУ (0- записать, 1 - загрузить в ОЗУ)
                                {0x00},                  //31 - фиксированный ток (мА) (F тип)
                                {0x00},                  //32 - измеренный ток (мА) при установке "0" ЦАП
                                {0x00},                  //33 - измеренный ток (мА) при установке коэфф. усиления ЦАП
                                {0x00},                  //34 - код функции преобразования (команда 24 будет использоваться)
                                {0x00},                  //35 - специфический статус устройства 0-5 байт (В тип)
                                {0x00},                  //36 - специфический статус устройства 14-24 байт (В тип)
                                {0x00},                  //37 - тэг устройства(?), 8 символов (А тип)
                                {0x00},                  //38 - дескриптор тэга устройства(?), 16 символов (А тип)
                                {0x00},                  //39 - дата устройства(?)(D тип)
                                {0x00},                  //40 - команда для работы в пакетном режиме
                                {0x00},                  //41 - контроль пакетного режима (0-выход, 1 - вход)
                                {0x00},                  //42 - "254", расширение
                                {0x00},                  //43 - любой не использующийся байт команды, это нужно, чтобы целостность команды не была нарушена 
                               };
                                 
